
CREATE TABLE Unprovoked (
    Country VARCHAR(40) NOT NULL,
    Year FLOAT NOT NULL,
    Area VARCHAR(400) NOT NULL,
    Activity VARCHAR(400) NOT NULL,
    Sex VARCHAR(40) NOT NULL,
    Age VARCHAR(40) NOT NULL,
    Fatal_Y_N VARCHAR(400) NOT NULL,
    Month INT NOT NULL
 );
-- Creating the table for hemisphere dataset
 CREATE TABLE Hemisphere (
    hemisph VARCHAR(40),
    Country VARCHAR (40),
    PRIMARY KEY (hemisph, Country)
 );

